{
    "name": "tobischulz/laravel-favoritable",
    "description": "Adds an easy way to make every model favoritable",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Schulz",
            "email": "tobias@byte.software"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "7.x"
    },
    "autoload": {
        "psr-4": {
            "TobiSchulz\\Favoritable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TobiSchulz\\Favoritable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TobiSchulz\\Favoritable\\FavoritableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}